{
    "name": "neck care",
    "version": "0.9.0",
    "description": "neck care app",
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    },
    "permissions": [
        "tabs",
        "bookmarks",
        "http://*/*",
        "https://*/*",
        "unlimitedStorage"
    ],
    "content_scripts": [ {
        "js": [ "jquery.min.js", "contentscript.js" ],
        "matches": [ "http://*/*", "https://*/*"]
    
    }],
    "background": {
        // "page": "background.html",
        "scripts": ["jquery.min.js", "background.js"]
    },
    "manifest_version": 2
}